{
  "hosting": {
    "source": ".",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "frameworksBackend": {
      "region": "us-central1",
      "id": "careconnect-app"
    },
    "rewrites": [
      {
        "source": "**",
        "apphosting": {
          "backendId": "careconnect-app",
          "region": "us-central1"
        }
      }
    ]
  },
  "apphosting": {
    "backendId": "my-web-app",
    "rootDir": "/",
    "ignore": [
      "node_modules",
      ".git",
      "firebase-debug.log",
      "firebase-debug.*.log",
      "functions"
    ]
  }
}
