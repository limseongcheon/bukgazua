{
  "hosting": {
    "source": ".",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "frameworksBackend": {
      "region": "us-central1"
    },
    "rewrites": [
      {
        "source": "**",
        "apphosting": {
          "backendId": "careconnect-app",
          "region": "us-central1"
        }
      }
    ]
  }
}
