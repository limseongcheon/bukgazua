# 관리자 계정 정보
CARECONNECT_ADMIN_USERNAME=admin
CARECONNECT_ADMIN_PASSWORD=password

# Resend 이메일 발송 정보 (샌드박스)
# API 키는 Resend 대시보드에서 발급받아 여기에 붙여넣으세요.
CARECONNECT_RESEND_API_KEY=
# 발신 이메일 주소는 Resend 샌드박스에서 허용된 주소여야 합니다. (보통 가입 시 사용한 이메일)
CARECONNECT_EMAIL_FROM=onboarding@resend.dev

# 수신자 이메일 주소 (최대 5개)
# Resend 샌드박스 모드에서는 이 주소 역시 발신 주소와 동일해야 이메일이 정상적으로 수신됩니다.
CARECONNECT_ADMIN_EMAIL_1=
CARECONNECT_ADMIN_EMAIL_2=
CARECONNECT_ADMIN_EMAIL_3=
CARECONNECT_ADMIN_EMAIL_4=
CARECONNECT_ADMIN_EMAIL_5=
